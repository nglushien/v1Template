- var tabs = ['Powerful Integration Testing & QA', 'Virtual Service Apps', 'Operational Efficiency', 'Infrastructure Integration'], graphicSize = '80x100', intoGraphicSize = '120x120';

.contentTabs
  .contentTabsNav
      ul
        for tab in tabs
          li: a(href='#') #{tab}
  .contentTabsPanelCollection
    each tab, index in tabs
      .contentTabsPanel
        .contentTabsPanelContent.oneColList
          .contentTabsPanelIntro
            h5 #{index+1} of #{tabs.length} images
            img(src="images/bannerAd1.png")
            img(src="images/bannerAd2.png")
          ul
            - for (var i = 0; i < 8; ++i) {
              li
                - for (var j = 0; j < 1; ++j) {
                  .mediaBlock.gridItem
                    .mediaBlockContent
                      h4: a(href='replaceMe') @Html.Ipsum.h4(4)
                      @Html.Ipsum.p(1, 4)
                      p: a.viewNow(href='replaceMe') > View Now
                - }
            - }